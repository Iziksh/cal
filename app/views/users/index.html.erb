<h1>Cat list</h1>

<table>
  <tr>
    <th>Id</th>
    <th>Name</th>
    <th>Gender</th>
  </tr>
  <% @cats.each do |cat| %>
    <tr>
      <td><%= cat.id %></td>
      <td><%= link_to(cat, cat) %></td>
      <td><%= cat.gender %></td>
      <td><%= link_to cat.user, cat.user if cat.user %></td>
      <td><%= link_to edit_cat_path(@cat) if @cat.user == current_user %></td>
    </tr>
  <% end %>
</table>

<%= link_to "New Cat", new_cat_path %>

