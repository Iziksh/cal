<h1>Employees list</h1>

<table>
  <tr>
    <th>Id</th>
    <th>Username</th>
  </tr>
  <% @Employees.each do |i| %>
    <tr>
      <td><%= Employee.id %></td>
      <td><%= link_to(i.username, i) %></td>
    </tr>
  <% end %>
</table>

<%= link_to 'New User', new_user_path %>
<%= "Signed in: #{user_signed_in?}" %>